{
  "schemaVersion": 1,
  "id": "lazyshuffle",
  "version": "${version}",
  "name": "LazyShuffle",
  "description": "Addon for SkinShuffle mod that allows you to load skins from the folder",
  "authors": [ "Fanya__" ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/lazyshuffle/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.fanya.lazyshuffle.client.LazyshuffleClient"
    ]
  },
  "mixins": [
    "lazyshuffle.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "skinshuffle": "*"
  }
}
